{
  "version": "1.6.2",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "diagnostics": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "isolatedModules": false,
    "listFiles": true,
    "noImplicitAny": false,
    "noLib": false,
    "noResolve": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true
  },
  "filesGlob": [
    "./**/*{.ts, .d.ts}",
    "../typings/tsd.d.ts",
    "../typings/**/*.d.ts",
    "../node_modules/typescript/lib/typescript.d.ts"
  ],
  "files": [
    "../typings/tsd.d.ts",
    "../node_modules/typescript/lib/typescript.d.ts",
    "./references.d.ts",
    "../typings/chalk/chalk.d.ts",
    "../typings/glob/glob.d.ts",
    "../typings/lodash/lodash.d.ts",
    "../typings/minimatch/minimatch.d.ts",
    "../typings/node/node.d.ts",
    "../typings/vinyl/vinyl.d.ts",
    "./DependencyBuilder.ts",
    "./DiagnosticsReporter.ts",
    "./EventEmitter.ts",
    "./Glob.ts",
    "./Logger.ts",
    "./BundleParser.ts",
    "./BundleCompiler.ts",
    "./TsCore.ts",
    "./TsProject.ts",
    "./TsVinylFile.ts",
    "./Utilities.ts",
    "./Compiler.ts",
    "./CompilerError.ts",
    "./CompilerHost.ts",
    "./CompilerReporter.ts",
    "./CompilerResult.ts",
    "./CompilerStatistics.ts",
    "./CompileStream.ts",
    "./Project.ts"
  ],
  "bundles": {
    "app": {
      "files": [
        "tsproject.ts",
        "references.d.ts"
      ]
    }
  }
}